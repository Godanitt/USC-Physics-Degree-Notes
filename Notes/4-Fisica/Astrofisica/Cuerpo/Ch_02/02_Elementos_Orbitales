                          <mrow class="MJX-TeXAtom-ORD">
                            <mn>2</mn>
                          </mrow>
                        </msup>
                      </msqrt>
                    </mrow>
                  </mrow>
                  <mo>)</mo>
                </mrow>
              </mtd>
            </mtr>
            <mtr>
              <mtd>
                <mi>ω<!-- ω --></mi>
              </mtd>
              <mtd>
                <mi></mi>
                <mo>=</mo>
                <mi>arg</mi>
                <mo>⁡<!-- ⁡ --></mo>
                <mrow>
                  <mo>(</mo>
                  <mrow>
                    <msub>
                      <mi>y</mi>
                      <mrow class="MJX-TeXAtom-ORD">
                        <mn>3</mn>
                      </mrow>
                    </msub>
